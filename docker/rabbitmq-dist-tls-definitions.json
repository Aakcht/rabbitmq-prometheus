{
  "global_parameters": [
    {"name": "cluster_name", "value": "rabbitmq-dist-tls"}
  ],
  "permissions": [
    {
      "configure": ".*",
      "read": ".*",
      "user": "guest",
      "vhost": "/",
      "write": ".*"
    }
  ],
  "policies": [
    {
      "apply-to": "queues",
      "definition": {"ha-mode": "nodes", "ha-params": ["rabbit@rabbitmq-dist-tls0","rabbit@rabbitmq-dist-tls1"]},
      "name": "ha0.1",
      "pattern": "ha0.1*",
      "priority": 0,
      "vhost": "/"
    },
    {
      "apply-to": "queues",
      "definition": {"ha-mode": "nodes", "ha-params": ["rabbit@rabbitmq-dist-tls0","rabbit@rabbitmq-dist-tls2"]},
      "name": "ha0.2",
      "pattern": "ha0.2*",
      "priority": 0,
      "vhost": "/"
    },
    {
      "apply-to": "queues",
      "definition": {"ha-mode": "exactly", "ha-params": 2},
      "name": "ha2",
      "pattern": "ha2.*",
      "priority": 0,
      "vhost": "/"
    },
    {
      "apply-to": "queues",
      "definition": {"ha-mode": "exactly", "ha-params": 3},
      "name": "ha3",
      "pattern": "ha3.*",
      "priority": 0,
      "vhost": "/"
    }
  ],
  "users": [
    {
      "hashing_algorithm": "rabbit_password_hashing_sha256",
